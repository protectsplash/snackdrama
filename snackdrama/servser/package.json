    "playground": "graphql playground",
    "dev": "npm-run-all --parallel start playground"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "graphql-yoga": "1.17.4",
    "jsonwebtoken": "8.5.1",
    "prisma-binding": "2.3.10"
  },
  "devDependencies": {
    "dotenv": "7.0.0",
    "graphql-cli": "3.0.11",
    "nodemon": "1.19.1",
    "npm-run-all": "4.1.5"
  }
}
